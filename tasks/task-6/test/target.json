{
  "application": {
    "name": "frontend-app",
    "version": "2.0.1",
    "enabled": true,
    "limits": {
      "memory": 512,
      "cpu": 1.5
    },
    "servers": [
      {
        "host": "localhost",
        "ports": [3000, 3001],
        "ssl": {
          "enabled": false,
          "cert": null
        }
      },
      {
        "host": "prod.example.com",
        "ports": [80, 443],
        "ssl": {
          "enabled": true,
          "cert": "/etc/ssl/cert.pem"
        }
      }
    ],
    "features": {
      "auth": {
        "enabled": true,
        "providers": ["google", "github"]
      },
      "experiments": {
        "enabled": false,
        "variants": []
      }
    },
    "meta": {
      "createdAt": "2025-01-01",
      "tags": ["stable", "v2"],
      "owner": null
    }
  }
}
